    <script>
        // 获取元素
        const box = document.querySelector(".view");
        const item = document.querySelector(".view-item");
        // 禁止图片拖拽
        const scrollItems = document.querySelectorAll(".scroll img");
        scrollItems.forEach(item => item.setAttribute("draggable", "false"));

        let itemWidth = item.clientWidth;
        // 原点坐标
        box.scrollLeft = itemWidth * 3;

        // 监听盒子滚动
        let boxScroll = function (event)
        {
            if (!event instanceof Event) return;
            let pos = box.scrollLeft - itemWidth * 3;
            if (Math.abs(pos) === itemWidth * 3)
            {
                // 回到原点
                box.classList.remove("smooth");
                box.scrollTo(itemWidth * 3, 0);
            }
        }

        // 事件注册盒子滚动
        box.addEventListener("scroll", boxScroll);

        // 鼠标按下
        let mouseDown = function (event)
        {
            // 移除过渡
            if (!event instanceof MouseEvent) return;
            // console.log("down");
            // 是否是在滑动的元素上 移除监听
            if (event.target.classList.item(0) === "scroll")
            {
                box.classList.remove("smooth");
                // console.log(event.target.classList.item(0));
                window.addEventListener("mousemove", mouseMove);
            }
            else
            {
                // console.log("class is null");
                window.removeEventListener("mousemove", mouseMove);
            }
        }
        // 鼠标抬起
        let mouseUp = function (event)
        {
            // 添加过渡
            if (!event instanceof MouseEvent) return;
            // console.log("up");
            box.classList.add("smooth");
            // 返回方向
            window.removeEventListener("mousemove", mouseMove);
            box.scrollLeft = next();

        }
        // 鼠标移动
        let mouseMove = function (event)
        {
            if (event instanceof MouseEvent)
            {
                // 限制最大滑动
                let moveX = Math.min(Math.abs(event.movementX), 100);
                direction = event.movementX > 0 ? -moveX : moveX;
                // 移动
                box.scrollBy(direction, 0);
            }
        }

        // 事件注册
        window.addEventListener("mousedown", mouseDown);
        window.addEventListener("mouseup", mouseUp);
        window.addEventListener("blur", e => window.removeEventListener("mousemove", mouseMove));

        // 自动回滚方法
        function next()
        {
            let current = box.scrollLeft % itemWidth;

            let target = 0;
            // 求余，得到哪边的距离较近
            if (current <= itemWidth / 2)
            {
                target = box.scrollLeft - current;
            }
            else
            {
                target = itemWidth - current + box.scrollLeft;
            }
            // 返回
            return target;
        }

        // 窗口改变监听
        window.addEventListener("resize", function (e)
        {
            // 重新计算盒子宽度
            itemWidth = item.clientWidth;
            // 回到原点
            box.scrollLeft = itemWidth * 3;
        })

        // 移动端开始-拖-结束 测试
        window.addEventListener("touchstart", e => box.classList.remove("smooth"));
        // window.addEventListener("touchstart", function (event)
        // {
        //     console.log("touchstart");
        // })
        window.addEventListener("touchend", function ()
        {
            box.classList.add("smooth");
            box.scrollLeft = next();
        })


    </script>
