    <style>
        :root {
            --title-font-size: 40px;
            --content-font-size: 15px;
            --mask-opacity: 0.3;
        }

        :root.dark {
            /* 深色模式 */
            --background-color: rgb(19, 19, 19);
            --logo-color: #eff2f5;
            --title-color: #fff;
            --content-color: rgb(168, 167, 167);
            --user-infos-color: rgba(116, 116, 116, 0.2);
        }

        :root.light {
            /* 浅色模式 */
            --background-color: #eff2f5;
            --logo-color: #08acf3;
            --title-color: rgb(0, 0, 0);
            --content-color: rgb(27, 27, 27);
            --user-infos-color: rgba(121, 121, 121, 0.2);
        }

        .smooth {
            scroll-behavior: smooth;
        }

        body {
            background: var(--background-color);
            transition: background-color 0.3s linear;
        }
